pragma solidity 0.8.9;
contract Whitelist {
   struct Person { // Structure de donn√©es
        string name;
        uint age;   
   }
   
    Person[] public persons; // Tableau dynamique de type Person
    function add (string memory _name, uint _age) public {
      Person memory person;
      person.name = "_name";
      person.age = _age;
      persons.push(person);

   }
   function remove (string memory _name, uint _age) public {
       persons.pop();
   }
}
